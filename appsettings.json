{
  "Serilog": {
    "Using":  ["Serilog.Sinks.File"],
    "MinimumLevel": "Debug",
    "WriteTo": [    
      { "Name": "File", "Args": { "path": "logs/log.txt" } }
    ]
  },
  "ServiceSettings":
  {
    "workingFolder":"work/",
    "filesFolder": "files/",
    "keywordsFolder": "files/keywords/",
    "deleteworkingfiles": true,
    "savetoazure": false,
    "savetolog": true,
    "maxattributescore": 100,
    "maxformulascore":100,
    "saveparsedcontent": true
  },
  "AllowedHosts": "*"
}




