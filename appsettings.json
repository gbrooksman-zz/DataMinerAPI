{
  "Serilog": {
    "Using":  ["Serilog.Sinks.File"],
    "MinimumLevel": "Information",
    "WriteTo": [    
      { "Name": "File", "Args": { "path": "logs/log.txt" } }
    ]
  },
  "ServiceSettings":
  {
    "workingFolder":"work/",
    "filesFolder": "files/",
    "deleteworkingfiles": true
  },
  "AllowedHosts": "*"
}

